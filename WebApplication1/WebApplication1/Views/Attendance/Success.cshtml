
@{    
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Success</title>
</head>
<body>
   <h3>attendance list</h3>
    <table cellpadding="2" cellspacing="2" border="1">
        <tr>
            <th>MemID</th>
        </tr>
        @foreach(var atd in ViewBag.lsatd)
        {
        <tr>
            <td>@atd.MemberID</td>
            <td>@atd.SessionID</td>
            <td>@atd.Status</td>
            <td>@atd.Note</td>
        </tr>
        }
    </table>
 </body>
</html>
